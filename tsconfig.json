{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "noEmit": true,
    "strict": false,
    "allowImportingTsExtensions": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "types": []
  },
  "files": ["./index.ts"],
  "exclude": ["node_modules", "./node_modules", "./node_modules/*"],
  "ts-node": {
    "esm": true
  }
}
